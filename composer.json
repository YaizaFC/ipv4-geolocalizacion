{
    "name": "yaiza/ipv4_geolocalizacion",
    "authors": [
        {
            "name": "yaizaFC",
            "email": "yaizafritis@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "ipinfo/ipinfo": "^1.1",
        "foolz/inet": "^0.6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "chmod -R 777 public/"
        ]
    }

}
